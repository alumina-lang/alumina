//! Helper functions used by the generated libc bindings.
//!
//! These provide Alumina equivalents of Rust std functions that
//! the libc crate's macro-expanded functions reference.

use std::mem::size_of;
use std::mem::zeroed;
use std::mem::uninitialized;

fn size_of_val<T>(_: &T) -> usize {
    size_of::<T>()
}
fn null_mut<T>() -> &mut T {
    null
}

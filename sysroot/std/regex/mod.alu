        set.add_interval('e' as u32, 'g' as u32);
        set.add_interval('b' as u32, 'f' as u32); // Overlaps both

        assert_eq!(set.intervals.len(), 1);
        assert_eq!(set.intervals[0].start, 'a' as u32);

    #[test]
    fn test_hashmap_borrowed() {
        use std::string::StringBuf;

        let map = HashMap::new::<StringBuf, i32>();
        defer map.free_all();

        map.insert(StringBuf::from_slice("hello"), 42);
        map.insert(StringBuf::from_slice("world"), 420);

        assert_eq!(*map.get_borrowed_ref("world").unwrap(), 420);
        *map.get_borrowed_mut("hello").unwrap() = 24;

        assert_eq!(map.get_borrowed("hello"), Option::some(24));
        let (k, _) = map.remove_borrowed("world").unwrap();
        assert_eq!(k[..] as &[u8], "world");
        k.free();

        assert_eq!(map.len(), 1);
    }

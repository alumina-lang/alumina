    #[test]
    fn test_hashmap_iter() {
        let map = HashMap::new::<i32, i32, DummyHasher>();
        defer map.free();

        map.insert(1, 2);
        map.insert(2, 4);
        map.insert(3, 5);

        let iter = map.iter();
        assert_eq!(iter.size_hint(), Option::some(3usize));
        assert!(iter.next() == Option::some((1, 2)));
        assert!(iter.next() == Option::some((2, 4)));
        assert!(iter.next() == Option::some((3, 5)));
        assert_eq!(iter.size_hint(), Option::some(0usize));
        assert!(iter.next() == Option::none());

        let iter = map.iter().rev();
        assert_eq!(iter.size_hint(), Option::some(3usize));
        assert!(iter.next() == Option::some((3, 5)));
        assert!(iter.next() == Option::some((2, 4)));
        assert!(iter.next() == Option::some((1, 2)));
        assert_eq!(iter.size_hint(), Option::some(0usize));
        assert!(iter.next() == Option::none());
    }

    #[test]
    fn test_hashmap_iter_ref() {
        let map = HashMap::new::<i32, i32, DummyHasher>();
        defer map.free();

        map.insert(1, 2);
        map.insert(2, 4);
        map.insert(3, 5);

        let iter = map.iter_ref();
        assert_eq!(iter.size_hint(), Option::some(3usize));
        let v = iter.next().unwrap();
        assert_eq!(*v.0, 1);
        assert_eq!(*v.1, 2);
        let v = iter.next().unwrap();
        assert_eq!(*v.0, 2);
        assert_eq!(*v.1, 4);
        let v = iter.next().unwrap();
        assert_eq!(*v.0, 3);
        assert_eq!(*v.1, 5);
        assert_eq!(iter.size_hint(), Option::some(0usize));

        assert!(iter.next().is_none());

        let iter = map.iter_ref().rev();
        assert_eq!(iter.size_hint(), Option::some(3usize));
        let v = iter.next().unwrap();
        assert_eq!(*v.0, 3);
        assert_eq!(*v.1, 5);
        let v = iter.next().unwrap();
        assert_eq!(*v.0, 2);
        assert_eq!(*v.1, 4);
        let v = iter.next().unwrap();
        assert_eq!(*v.0, 1);
        assert_eq!(*v.1, 2);
        assert_eq!(iter.size_hint(), Option::some(0usize));

        assert!(iter.next().is_none());
    }

    #[test]
    fn test_hashmap_iter_mut() {
        let map = HashMap::new::<i32, i32, DummyHasher>();
        defer map.free();

        map.insert(1, 2);
        map.insert(2, 4);
        map.insert(3, 5);

        let iter = map.iter_mut();
        assert_eq!(iter.size_hint(), Option::some(3usize));
        let v = iter.next().unwrap();
        assert_eq!(*v.0, 1);
        assert_eq!(*v.1, 2);
        *v.1 = 100;

        let iter = map.iter_mut().rev();
        assert_eq!(iter.size_hint(), Option::some(3usize));
        let v = iter.next().unwrap();
        assert_eq!(*v.0, 3);
        assert_eq!(*v.1, 5);
        *v.1 = 200;

        assert_eq!(map.get(&1).unwrap(), 100);
        assert_eq!(map.get(&3).unwrap(), 200);
    }

    #[test]
    fn test_reserve() {
        let map = HashMap::new::<i32, i32>();
        defer map.free();

        map.reserve(100);

        let ptr = &map._buckets[0];
        for i in 0..100 {
            map.insert(i, i);
        }

        // Did not reallocate
        assert_eq!(&map._buckets[0], ptr);
    }


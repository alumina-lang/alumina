use std::io::println;

fn is_prime(v: i64) -> bool {
    let i: i64 = 2;
    while i < v {
        if v % i == 0 {
            return false;
        }
        i += 1;
    }
    true
}

#[export]
fn main() {
    let i: i64 = 1;

    macro inc() {
        i += 1;
    }

    while i < 100 {
        if is_prime(i) {
            println!("{}", i);
        }
        inc!();
    }
}

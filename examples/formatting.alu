struct Foo {
    a: i32, 
    b: i32
}

impl Foo {
    use std::fmt::{Formatter, write};

    fn fmt<F: Formatter<F>>(self: &Foo, formatter: &mut F) {
        write!(formatter, "Foo {{ a: {}, b: {} }}", self.a, self.b);
    }
}

fn main() {
    println!("{}", Foo { a: 1, b: 2 });
    println!("{}", Foo { a: 2, b: 3 });
}

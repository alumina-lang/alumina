fn is_prime(v: i64) -> bool {
    let i: i64 = 2;
    while i < v {
        if v % i == 0 {
            return false;
        }
        i += 1;
    }
    true
}

fn print_filtered(lower: i64, upper: i64, filter: fn(i64) -> bool) {
    while lower < upper {
        if filter(lower) {
            println!("{}", lower)
        }
        lower += 1;
    }
}

fn main() {
    print_filtered(1, 100, |p| p < 10);
    print_filtered(1, 100, |p| is_prime(p));
}

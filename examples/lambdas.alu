use std::iter::Range;

fn is_prime(v: i64) -> bool {
    let i: i64 = 2;
    while i < v {
        if v % i == 0 {
            return false;
        }
        i += 1;
    }
    true
}

fn print_filtered(range: Range<i64>, filter: fn(i64) -> bool) {
    for i in range {
        if filter(i) {
            println!("{}", i)
        }
    }
}

fn main() {
    print_filtered(1..100, |p| p < 10);
    print_filtered(1..100, |p| is_prime(p));
}

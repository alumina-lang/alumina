macro compile_fail($reason) {
    intrinsics::compile_fail($reason);
}

macro assert($cond) {
    if !$cond {
        panic!("assertion failed");
    }
}

macro debug_assert($cond) {
    #[cfg(debug)]
    assert!($cond);
}

#[builtin] macro line() {  }
#[builtin] macro column() {  }
#[builtin] macro file() {  }
#[builtin] macro env($s) {  }
#[builtin] macro include_bytes($file) {  }
#[builtin] macro concat($parts...) {  }

mod util {
    #[force_inline]
    fn identity<T>(t: T) -> T { t }
}
